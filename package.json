{
    "name": "github_users",
    "version": "0.0.1",
    "author": "Robert",
    "private": true,
    "description": "Github user search. A React application using Github API v3.",
    "keywords": [
        "react",
        "typescript",
        "redux",
        "redux-saga"
    ],
    "homepage": "https://github.com/primalvinyl/react-github-user-search#readme",
    "repository": {
