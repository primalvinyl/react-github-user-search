        "@types/react-table": "6.8.5",
        "@types/yup": "0.26.24",
        "@typescript-eslint/eslint-plugin": "2.6.1",
        "@typescript-eslint/parser": "2.6.1",
        "babel-jest": "24.9.0",
        "babel-loader": "8.0.6",
