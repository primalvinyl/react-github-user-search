        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:snapshot": "jest --updateSnapshot",
        "test:clear": "jest --clearCache",
        "lint": "./node_modules/.bin/eslint \"src/**\"",
