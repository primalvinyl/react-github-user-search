        "file-loader": "4.2.0",
        "html-webpack-plugin": "3.2.0",
        "jest": "^26.4.2",
        "mini-css-extract-plugin": "0.8.0",
        "node-sass": "^4.14.1",
        "pre-commit": "1.2.2",
        "redux-saga-test-plan": "4.0.0-rc.1",
        "rimraf": "3.0.0",
        "sass-loader": "7.3.1",
        "style-loader": "1.0.0",
        "ts-jest": "24.1.0",
        "ts-loader": "6.2.0",
        "typescript": "3.6.3",
        "url-loader": "2.2.0",
        "webpack": "4.39.3",
        "webpack-cli": "3.3.8",
