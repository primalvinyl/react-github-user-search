        "react-router-dom": "5.0.1",
        "react-table": "^7.5.1",
        "redux": "4.0.4",
        "redux-devtools-extension": "2.13.8",
        "redux-saga": "1.0.5",
        "yup": "0.27.0"
    },
    "pre-commit": {
        "run": [
            "message-pre-commit",
            "test",
            "lint"
        ]
    }
